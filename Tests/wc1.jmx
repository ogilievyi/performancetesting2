            <stringProp name="script">import groovy.json.JsonSlurper
import org.apache.commons.io.FileUtils;

var response = prev.getResponseDataAsString();
def slurper = new JsonSlurper()
def jsonData = slurper.parseText(response);
var minTime = 99999999999999999999;
var maxTime = 0;
var minTimeStr = &apos;&apos;;
var maxTimeStr = &apos;&apos;;

for ( r in  jsonData.requests) {
	if(minTime &gt; r.request.loggedDate){
	      minTime = r.request.loggedDate;
	      minTimeStr = r.request.loggedDateString;
	  }
	  if(maxTime &lt; r.request.loggedDate){
	      maxTime = r.request.loggedDate;
	      maxTimeStr = r.request.loggedDateString;
	  }
}

FileUtils.writeStringToFile(new java.io.File(&quot;D:/GSchool/performancetesting2/Tests/test.log&quot;), 
     &quot;minTimeStr: &quot; + minTimeStr + &quot;\r\nmaxTimeStr: &quot; + maxTimeStr + &quot;\r\nduration: &quot; + (maxTime - minTime), 
     &quot;UTF-8&quot;, 
      false);
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PostProcessor>
